{
  // Synology module config
  "Synology": {
    "host": {
      "protocol": "http",
      "url": "192.168.0.x",
      "port": "5000"
    },
    "account": {
      "user": "user",
      "password": "password"
    }
  },
	// The Movie Database config
  "MovieDB": {
    "api": {
      "key": "api_key"
    }
  },
  // MongoDB config
  "MongoDB": {
    "uri": "mongodb://<dbuser>:<dbpassword>@<dbhost>:<dbport>/<dbdatabase>"
  },
  // Folder for process
  "Process": {
    "folder": {
      "downloads": "",
      "tv": "",
      "movie": ""
    }
  },
  // Basic Auth config
  "BasicAuth": {
    "enable": true|false,
    "user": "user",
    "password": "password"
  }
}